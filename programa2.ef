int main(){
	
	let x : int;
	let somas : int[4];

	read(x);
	
	while(x>0){
		
		if(x>0 && x<=25){
			somas[0]++;
		}else{
			if(x>=26 && x<=50){
			somas[1]++;
		}else{
			if(x>=51 && x<=75){
				somas[2]++;
			}else{
				if(x>=76 && x<=100){
					somas[3]++;
				}
			}
		}		
		read(x);
	}

	print(somas);

	return 0;
}